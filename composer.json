{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.3",
        "ext-iconv": "*",
        "easycorp/easyadmin-bundle": "^2.3",
        "fabpot/goutte": "^4.0",
        "symfony/console": "^5.0",
        "symfony/dom-crawler": "^5.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "rector/rector": "^0.7.3",
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "^5.0",
        "symfony/maker-bundle": "^1.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "php-cs-fixer/diff": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
