{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/common": "^3.3",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.12",
        "easycorp/easyadmin-bundle": "^4.0",
        "fabpot/goutte": "^4.0",
        "symfony/console": "^6.0",
        "symfony/dom-crawler": "^6.0",
        "symfony/flex": "^2.1",
        "symfony/framework-bundle": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "rector/rector": "^0.12",
        "roave/security-advisories": "dev-latest",
        "symfony/dotenv": "^6.0",
        "symfony/maker-bundle": "^1.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "php-cs-fixer/diff": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
